@using WebApplication1.Models.Siniflar
@model List<User>

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<title>W3.CSS</title>
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />

<body>

    <!-- Sidebar -->
    <div class="w3-sidebar w3-light-grey w3-bar-block" style="width:15%">
        <h3 class="w3-bar-item">Menu</h3>
        <a href="/Map/Map/" class="w3-bar-item w3-button">Harita</a>
        <a href="/Calender/Get/" class="w3-bar-item w3-button">Takvim</a>
        <a href="/Login/LogOut/" class="w3-bar-item w3-button">Çıkış</a>
    </div>

    <!-- Page Content -->
    <div style="margin-left:15%">

        <div class="w3-container w3-teal">
            <h4>Admin Yönetim Paneli</h4>
        </div>


        <div class="w3-container">
            @RenderBody()
        </div>

    </div>

</body>
</html>



